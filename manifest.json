{
  "manifest_version": 3,
  "name": "Braid Chrome Extension",
  "version": "0.0.155",
  "permissions": [
    "activeTab",
    "tabs",
    "storage",
    "scripting",
    "background",
    "webRequest"
  ],
  "devtools_page": "devtools.html",
  "host_permissions": [
    "https://*/*",
    "http://*/*",
    "file:///*"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "16.png",
      "48": "48.png",
      "128": "128.png"
    }
  },
  "icons": {
    "16": "16.png",
    "48": "48.png",
    "128": "128.png"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "dt_bg.wasm",
        "inject.html",
        "braid-http-client.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "./content_script.js",
        "./braid-http-client.js"
      ]
    }
  ],
  "background": {
    "service_worker": "background.js"
  }
}
